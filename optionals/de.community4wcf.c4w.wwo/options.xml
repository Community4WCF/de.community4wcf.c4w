<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/options.xsd">
	<import>
		<categories>
			<category name="c4w.index.wwo">
				<parent>c4w.index</parent>
			</category>
		</categories>
		<options>
			<option name="index_enable_wasonline_list">
				<categoryname>c4w.index.wwo</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<enableoptions>index_limit_wasonline_list,index_wasonline_list_showpermanent,index_wasonline_list_hidetime</enableoptions>
