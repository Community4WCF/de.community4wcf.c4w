			<option name="index_wasonline_list_hidetime">
				<categoryname>c4w.index.wwo</categoryname>
				<optiontype>boolean</optiontype>
			</option>

			<option name="users_wasonline_record">
				<categoryname>c4w.index.wwo</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>33</defaultvalue>
				<hidden>1</hidden>
			</option>
			<option name="users_wasonline_record_time">
				<categoryname>c4w.index.wwo</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>1284413542</defaultvalue>
